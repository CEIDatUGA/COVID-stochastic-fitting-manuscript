	keywords = {R},
	pages = {1--43},
	file = {Full Text:C\:\\Users\\atredennick\\Zotero\\storage\\9BEW4QYP\\King et al. - 2016 - Statistical Inference for Partially Observed Marko.pdf:application/pdf},
}

