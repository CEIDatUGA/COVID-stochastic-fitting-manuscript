	keywords = {Humans, COVID-19, SARS-CoV-2, Pandemics, Basic reproduction number, Basic Reproduction Number, Betacoronavirus, Coronavirus Infections, Global Health, Meta-analysis, Outbreak, Pneumonia, Viral, Public health},
	pages = {151--157},
