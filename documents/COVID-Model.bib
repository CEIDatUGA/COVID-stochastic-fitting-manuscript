@article{castle_value_2021,
	title = {{THE} {VALUE} {OF} {ROBUST} {STATISTICAL} {FORECASTS} {IN} {THE} {COVID}-19 {PANDEMIC}},
	volume = {256},
	issn = {0027-9501, 1741-3036},
	url = {https://www.cambridge.org/core/journals/national-institute-economic-review/article/abs/value-of-robust-statistical-forecasts-in-the-covid19-pandemic/BF920D207E503951726B636D35217530},
	doi = {10.1017/nie.2021.9},
	abstract = {The Covid-19 pandemic has put forecasting under the spotlight, pitting epidemiological models against extrapolative time-series devices. We have been producing real-time short-term forecasts of confirmed cases and deaths using robust statistical models since 20 March 2020. The forecasts are adaptive to abrupt structural change, a major feature of the pandemic data due to data measurement errors, definitional and testing changes, policy interventions, technological advances and rapidly changing trends. The pandemic has also led to abrupt structural change in macroeconomic outcomes. Using the same methods, we forecast aggregate UK unemployment over the pandemic. The forecasts rapidly adapt to the employment policies implemented when the UK entered the first lockdown. The difference between our statistical and theory based forecasts provides a measure of the effect of furlough policies on stabilising unemployment, establishing useful scenarios had furlough policies not been implemented.},
	language = {en},
	urldate = {2022-03-29},
	journal = {National Institute Economic Review},
	author = {Castle, Jennifer L. and Doornik, Jurgen A. and Hendry, David F.},
	year = {2021},
	note = {Publisher: Cambridge University Press},
	keywords = {C22, C53, Covid-19, E27, epidemiology, structural breaks, time-series forecasting, unemployment},
	pages = {19--43},
	file = {Snapshot:C\:\\Users\\atredennick\\Zotero\\storage\\YC885S7C\\BF920D207E503951726B636D35217530.html:text/html;Submitted Version:C\:\\Users\\atredennick\\Zotero\\storage\\Q936SUHI\\Castle et al. - 2021 - THE VALUE OF ROBUST STATISTICAL FORECASTS IN THE C.pdf:application/pdf},
